<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Plus+Jakarta+Sans&display=swap"
      rel="stylesheet"
    />
    <link rel="stylesheet" href="timeline.css" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="timeline.js"></script>
    <title>Document</title>
  </head>
  <body>
    <div id="outer">
      <header>
        <img class="partials" src="header.png" alt="" />
      </header>

      <nav>
        <div id="dummy_div">
          <a href="/index"> Home Page </a>
          <a href="/search_page"> Search page </a>

          <a href="/timeline"> Timeline/Profile  </a>

          <a href="/oldOrder"> Previous Orders </a>

          <a href="/personalCart"> Cart ðŸ›’ </a>
        </div>

        <form action="/logout" method="POST">
          <button class="logoutButton">Logout</button>
        </form>
      </nav>
      <div class="mainContent">
        <div class="TimelineContent">
          <h1>Global timeline</h1>

          <main></main>
        </div>
        <div class="profileContent">
          <h1>Profile</h1>
          <div class="profileInfo">
            <%userList.forEach(users => {%>
            <div>
              <img src="profile.png" alt="" />
            </div>
            <div class="info">
              <h3>Username: <%= users.username %></h3>
              <h3>Email: <%= users.email %></h3>
            </div>
            <%})%>
          </div>
        </div>
      </div>

      <footer>
        <h5>Copyright Â© Your Website 2022</h5>
        <img class="partials" src="footer.png" alt="" />
      </footer>
    </div>
  </body>
</html>
